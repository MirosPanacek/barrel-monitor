{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "measurement.schema.json",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "barrelId": {
      "type": "string",
      "format": "uuid"
    },
    "dirtLevel": {
      "type": "integer",
      "minimum": 0
    },
    "weight": {
      "type": "integer",
      "minimum": 0
    }
  },
  "required": ["barrelId", "dirtLevel", "weight"],
  "additionalProperties": false
}
